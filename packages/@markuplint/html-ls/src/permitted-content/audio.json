{
	"if": {
		"hasAttr": "src"
	},
	"then": [
		{
			"count": "zero-or-more",
			"content": "track"
		},
		{
			"count": "zero-or-more",
			"content": {
				"either": [
					{
						"category": "transparent",
						"ignore": {
							"content": ["audio", "video"],
							"descendants": true
						}
					}
				]
			}
		}
	],
	"else": [
		{
			"count": "zero-or-more",
			"content": "source"
		},
		{
			"count": "zero-or-more",
			"content": "track"
		},
		{
			"count": "zero-or-more",
			"content": {
				"either": [
					{
						"category": "transparent",
						"ignore": {
							"content": ["audio", "video"],
							"descendants": true
						}
					}
				]
			}
		}
	]
}
